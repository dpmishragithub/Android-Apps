<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.praneeth.chitchat.SubscribedChannels">

    <TextView
        android:id="@+id/textView_YSubChan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/textView_YSubChan"
        android:textColor="@android:color/black" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recycler_view_channels"
        android:layout_width="match_parent"
        android:layout_height="431dp" />

    <Button
        android:id="@+id/button_AddMore"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/button_AddMore" />
</LinearLayout>
